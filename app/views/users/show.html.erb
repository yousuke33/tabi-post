<div class="container">
  <h1><%= @user.guest.name if @user.role == 'guest'%> 
    <%= @user.owner.name if @user.role == 'owner'%> 
    様のページ
  </h1>
  <h2><%= @user.email %></h2>
  <% if @user.role == "guest" %>
    <h3>プラン一覧</h3>
      <%= render partial: "guest", locals:{user: @user} %>
  <% end %>
</div>




