<div class="container">
  <h1>お客様用ページ</h1>  
  <div class="row">
    <div class="col-md-4">
      お名前: <%= @user.name %>
      <%= @user.role %>
    </div>
    <div class="col-md-8">
        <%= @plan.each do |plan| %> 
        <div class="box">
          <%= render partial: "/plans/item", locals:{plan: plan} %>
          </div>
        <% end %>    
    </div>
  </div>
</div>
